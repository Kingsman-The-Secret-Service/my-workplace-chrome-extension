{
	"name": "My Workplace",
	"version": "0.1",
	"description": "Its my place, No intruders",
	"manifest_version": 2,
	"permissions": [
		"bookmarks",
		"tabs",
		"activeTab",
		"declarativeContent",
		"storage"
	],
	"chrome_url_overrides":{
		"newtab": "html/redirect.html"
	},
	"browser_action": {
		"default_title": "My Workplace"
	},
	"background": {
		"scripts": ["js/background.js"],
		"persistent": false
	}
